{% extends 'index.html.twig' %}

{% block body %}
    <div class="container">
        <h1>UK words</h1>
        {% for word in uk_words %}
        {{word.getWord}}
        {% endfor %}

        <h1>US words</h1>
        {% for word in us_words %}
        <li>{{word.getWord}}</li>
        {% endfor %}
    </div>
{% endblock %}
